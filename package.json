{
  "name": "your-project",
  "version": "1.0.0",
  "description": "Project description",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ci": "npm ci && npm run lint && npm run type-check && npm test",
    "commit": "node scripts/git/parallel-commit.js",
    "commit:dry": "node scripts/git/parallel-commit.js --dry-run",
    "commit:sh": "bash scripts/git/parallel-commit.sh",
    "commit:sh:dry": "bash scripts/git/parallel-commit.sh --dry-run",
    "commit:all": "git add -A && git commit -m \"chore: commit all changes\"",
    "commit:auto": "bash scripts/git/parallel-commit.sh --max 3",
    "status": "git status",
    "changes": "git status --porcelain",
    "staged": "git diff --cached --name-only"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.0.0",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "mermaid": "^11.12.2",
    "next": "^14.0.0",
    "next-mdx-remote": "^5.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^10.1.0",
    "react-mermaid2": "^0.1.4",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
